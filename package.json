{
  "name": "shaclc-write",
  "version": "0.0.0",
  "description": "A writer for SHACLC documents",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "lint": "eslint lib/* __tests__/*.ts --ext .ts",
    "lint:fix": "eslint lib/* __tests__/*.ts --ext .ts --fix",
    "build": "tsc",
    "prepare": "tsc",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeswr/shaclc-writer.git"
  },
  "keywords": [
    "SHACLC",
    "SHACL-compact-syntax",
    "writer",
    "rdf"
  ],
  "author": "Jesse Wright <jesse.wright@anu.edu.au>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeswr/shaclc-writer/issues"
  },
  "homepage": "https://github.com/jeswr/shaclc-writer#readme",
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/n3": "^1.4.4",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "pre-commit": "^1.2.2",
    "semantic-release": "^19.0.5",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@rdfjs/types": "^1.1.0",
    "n3": "^1.6.4",
    "rdf-string-ttl": "^1.1.0"
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverage": true,
    "verbose": false,
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 100,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "pre-commit": [
    "lint",
    "build",
    "test"
  ],
  "release": {
    "branches": [
      "main"
    ]
  }
}
